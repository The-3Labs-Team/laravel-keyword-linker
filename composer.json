{
    "name": "the-3labs-team/laravel-keyword-linker",
    "description": "This is my package laravel-keyword-linker",
    "keywords": [
        "The-3Labs-Team",
        "laravel",
        "laravel-keyword-linker"
    ],
    "homepage": "https://github.com/the-3labs-team/laravel-keyword-linker",
    "license": "MIT",
    "authors": [
        {
            "name": "The-3Labs-Team",
            "email": "murdercode@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/contracts": "^10.0||^11.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "larastan/larastan": "^2.9",
        "orchestra/testbench": "^9.0.0||^8.22.0",
        "pestphp/pest": "^2.34",
        "pestphp/pest-plugin-arch": "^2.7",
        "pestphp/pest-plugin-laravel": "^2.3",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-phpunit": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "The3LabsTeam\\KeywordLinker\\": "src/",
            "The3LabsTeam\\KeywordLinker\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "The3LabsTeam\\KeywordLinker\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-laravel-keyword-linker --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/testbench workbench:build --ansi"
        ],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "@composer run build",
            "@php vendor/bin/testbench serve"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "The3LabsTeam\\KeywordLinker\\KeywordLinkerServiceProvider"
            ],
            "aliases": {
                "KeywordLinker": "The3LabsTeam\\KeywordLinker\\Facades\\KeywordLinker"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}